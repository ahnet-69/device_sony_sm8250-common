diff --git a/products/AndroidProducts.mk b/products/AndroidProducts.mk
index 9d61def..1616d40 100644
--- a/products/AndroidProducts.mk
+++ b/products/AndroidProducts.mk
@@ -35,6 +35,8 @@ PRODUCT_MAKEFILES += \
     $(LOCAL_DIR)/oneplus7t/aospa_oneplus7t.mk \
     $(LOCAL_DIR)/oneplus7tpro/aospa_oneplus7tpro.mk \
     $(LOCAL_DIR)/oneplus9/aospa_oneplus9.mk \
+    $(LOCAL_DIR)/pdx203/aospa_pdx203.mk \
+    $(LOCAL_DIR)/pdx206/aospa_pdx206.mk \
     $(LOCAL_DIR)/pdx234/aospa_pdx234.mk \
     $(LOCAL_DIR)/phoenix/aospa_phoenix.mk \
     $(LOCAL_DIR)/phone1/aospa_phone1.mk \
@@ -69,6 +71,8 @@ COMMON_LUNCH_CHOICES += \
     aospa_oneplus7t-userdebug \
     aospa_oneplus7tpro-userdebug \
     aospa_oneplus9-userdebug \
+    aospa_pdx203-userdebug \
+    aospa_pdx206-userdebug \
     aospa_pdx234-userdebug \
     aospa_phoenix-userdebug \
     aospa_phone1-userdebug \
diff --git a/products/pdx203/aospa_pdx203.mk b/products/pdx203/aospa_pdx203.mk
new file mode 100644
index 0000000..93b38a2
--- /dev/null
+++ b/products/pdx203/aospa_pdx203.mk
@@ -0,0 +1,45 @@
+# Copyright (C) 2022-2024 Paranoid Android
+#
+# Licensed under the Apache License, Version 2.0 (the "License");
+# you may not use this file except in compliance with the License.
+# You may obtain a copy of the License at
+#
+#      http://www.apache.org/licenses/LICENSE-2.0
+#
+# Unless required by applicable law or agreed to in writing, software
+# distributed under the License is distributed on an "AS IS" BASIS,
+# WITHOUT WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.
+# See the License for the specific language governing permissions and
+# limitations under the License.
+
+ifeq (aospa_pdx203, $(TARGET_PRODUCT))
+
+# Inherit from those products. Most specific first.
+$(call inherit-product, $(SRC_TARGET_DIR)/product/core_64_bit_only.mk)
+$(call inherit-product, $(SRC_TARGET_DIR)/product/aosp_base_telephony.mk)
+
+# Inherit from the custom device configuration.
+$(call inherit-product, device/sony/pdx203/device.mk)
+
+# Inherit from the AOSPA configuration.
+$(call inherit-product, vendor/aospa/target/product/aospa-target.mk)
+
+PRODUCT_NAME := aospa_pdx203
+PRODUCT_BRAND := Sony
+PRODUCT_DEVICE := pdx203
+PRODUCT_MANUFACTURER := Sony
+PRODUCT_MODEL := XQ-AT52
+
+PRODUCT_SYSTEM_NAME := XQ-AT52
+PRODUCT_SYSTEM_DEVICE := XQ-AT52
+
+PRODUCT_GMS_CLIENTID_BASE := android-sonymobile
+
+# Boot animation resolution.
+TARGET_BOOT_ANIMATION_RES := 1080
+
+PRODUCT_BUILD_PROP_OVERRIDES += \
+    TARGET_DEVICE=$(PRODUCT_SYSTEM_DEVICE) \
+    TARGET_PRODUCT=$(PRODUCT_SYSTEM_NAME)
+
+endif
